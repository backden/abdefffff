<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="V1/stagingFeature/labels" method="POST">
        <service class="Swatch\ManageLabel\Webservice\V1\Production\TranslateWebApiInterface" method="executeProductionWebService"/>
        <resources>
            <resource ref="Swatch_ManageLabel::Translate"/>
        </resources>
    </route>
</routes>
